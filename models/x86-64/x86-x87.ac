AC_ISA(x86)
{
	ac_format FORMAT_DUMMY_X87 = "%dummy:32";
	
	ac_instr<FORMAT_DUMMY_X87> fnstcw, fld, fst, fstp;
	
	ac_behaviour fnstcw, fld, fst, fstp;
	
	ISA_CTOR(x86) {
		fld.set_decoder();
		fld.set_behaviour(fld);
	
		fnstcw.set_decoder();
		fnstcw.set_behaviour(fnstcw);
		
		fst.set_decoder();
		fst.set_behaviour(fst);
		
		fstp.set_decoder();
		fstp.set_behaviour(fstp);
	
	};
};
