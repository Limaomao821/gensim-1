AC_ISA(x86)
{
	ac_fetchsize 8;
	
	include("x86-base.ac");

	struct Operand {
		uint8 is_reg;
		
		Register reg;
		Memory memory;
	};
	
	struct Register {
		uint8 index;
		uint8 width;
	};
	
	struct Memory {
		
	};
	
	ac_field<Operand> op0;
	ac_field<Operand> op1;
	
	ISA_CTOR(x86)
	{
		ac_behaviours("behaviours.x86");
		ac_execute("execute.x86-base");
	};
};
